#include<stdio.h> 
#include<conio.h> 

int  main()
{
   int n1,n2,n3,n4,n5,sum,total=500;
   float per;

   printf("Enter the Marks of 5 Subjects:");
   scanf("%d%d%d%d%d",&n1,&n2,&n3,&n4,&n5);

   sum= n1+n2+n3+n4+n5;
   printf("\nsum=%d",sum);

   per=(sum*100)/total;

   printf("\Percentage: %f", per);
   getch();
}
