#include<stdio.h>
int main()
{
int i,j,r,c,a[i][j],transpose[i][j];
printf("enter  number of rows and column");
scanf("%d",&r,&c);
printf("enter the first matrix elements");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
	printf("enter element a%d%d",i+1,j+1);
scanf("%d",&a[i][j]);
}
}
printf("enter the second matrix elements");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
scanf("%d",&a[i][j]);
}
}
if(j==c-1)
{
printf("\n");
}
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
	transpose[j][i]=a[i][j];
}
}
printf("transpose of matrix");
for(i=0;i<r;i++)
{
	for(j=0;j<c;j++)
{
printf("%d",&transpose[i][j]);
}
}
if(j==r-1)
{
printf("\n");
}
return 0;
}
